@model HealthJang.Models.Board

@{
    ViewBag.Title = "Detail";
}

<h2>Detail</h2>

<div class="form-horizontal">
    <div class="form-group">
        <!--@Html.LabelFor(m => m.BoardTitle)-->
        <label>제목</label>
        @Html.TextBoxFor(m => m.BoardTitle, new { @class = "form-control", disabled = "disabled"})
    </div>

    <div class="form-group">
        <!--@Html.LabelFor(m => m.BoardContents)-->
        <label>내용</label>
        @Html.TextAreaFor(m => m.BoardContents, new { @class = "form-control", rows = 20, disabled = "disabled" })
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-primary">등록하기</button>
        @Html.ActionLink("뒤로가기", "Index")
    </div>

</div>


